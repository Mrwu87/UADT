image:
  tag: {{ env "IMAGE_TAG" | default "0.3.1" }}
  repository: harbor.uisee.cn/cloud/scheduler-general

imagePullSecrets:
- name: docker-reg-secret

logging:
  enabled: true
  cluster: UcloudKubernetes

mqttConfig:
  generalNome: false

redisConfig:
  host: scheduler-redis-latest-master
  password: redis
  port: 6379

httpClientConfig:
  nome:
    host: ""
    port: ""

resources:
  limits:
    cpu: "0.5"
    memory: 1Gi
  requests:
    cpu: "0.25"
    memory: 500Mi


