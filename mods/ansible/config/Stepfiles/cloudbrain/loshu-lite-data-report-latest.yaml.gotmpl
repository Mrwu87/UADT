        csxm:
          enable: 1
          single_report_type: ['logistics']
          project_name: '测试项目'




env:
  elasticsearch_ip: elasticsearch-master
  elasticsearch_port: 9200

project: "dongwu"

resources:
  requests:
    memory: 500Mi
  limits:
    memory: 4Gi
