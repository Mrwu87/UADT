          compass_alert_api: http://console-notification-latest/notification/internal/records/search
          compass_odometers_api: http://compass-backend-latest:8000/odometers/
          compass_statistic_api: http://console-notification-latest/notification/statistics
          compass_error_codes_api: http://compass-backend-latest:8000/diagnoses/{}/error_codes/
          console_project_api: http://console-core-latest:80/groups
          console_map_api: http://console-map-latest:80/internal/maps
          console_vehicle_api: http://console-vehicle-latest:80/vehicles
          compass_influxdb_api: http://emqx-influxdb-latest:8086/query
          compass_parking_history_api: http://compass-backend-latest:8000/parkings/{}/histories/
          compass_parking_stattistic_api: http://compass-backend-latest:8000/parkings/statistic/
          console_situ_mapstops_api: http://situ-map-proxy-latest:80/maps/{}/versions/{}/stops
