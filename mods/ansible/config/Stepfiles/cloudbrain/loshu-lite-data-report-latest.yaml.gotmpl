persistence:
  enabled: false
  storageClass: "managed-nfs-storage"

cronjob:
  # At 01:00 every day
  schedule: "5 1 * * *"

logging:
  enabled: true
  cluster: UcloudKubernetes

image:
  tag: {{ env "IMAGE_TAG" | default "0.3.0-report_lite.09aeef5-20201124" }}
  repository: harbor.uisee.cn/loshu/data-report

imagePullSecrets:
- name: docker-reg-secret

configurationFiles:
    dongwu.configuration.datareport.yml: |-
      common:
        program: logistics
        elasticsearch_ip: 10.23.120.78
        elasticsearch_port: 9200
        upload_url: http://s3-cn-sh2.ufileos.com
        upload_bucket: uisee-loshu
        access_key: TOKEN_78ebc873-888c-449b-b23d-15c997c402a6
        secret_key: 09f99b9f-2fb2-4bc0-924a-c62c5e280d0c
        upload_folder_name: jiashan-dongwu
        console_mode: inside
        report_save_days: 60
        log_mode: production1
        single_report_type: ['logistics']
        coord_report_type: ['logistics']
        coord_report_summary_days: [1, 7]
        base_stats: 1
        coord_report: 1
        excel_report: 1
        html_report: 0
        live_report: 0
        console_report: 1
        nome_plus: 1
        excel_select: 512
        ahead_day: 1
        ahead_day_cnt: 1
